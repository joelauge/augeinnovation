# Clerk.dev Authentication
# Get your publishable key from https://dashboard.clerk.dev
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_key_here

# Stripe Payments
# Get your publishable key from https://dashboard.stripe.com
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here

# Xero Integration (for Netlify Functions)
# Get your credentials from https://developer.xero.com/
XERO_CLIENT_ID=your_xero_client_id_here
XERO_CLIENT_SECRET=your_xero_client_secret_here
XERO_TENANT_ID=your_xero_tenant_id_here
XERO_REDIRECT_URI=https://yourdomain.com/auth/callback

# Email Configuration
ADMIN_EMAIL=sales@aiinnovation.com

# Optional: Backend API URL (if you have a backend)
# REACT_APP_API_URL=https://your-api.com 